(:, k) = fread(fip, [block_size, 1], 'float32');\n        fclose(fip);\n        k = k + 1;\n    end\n    ny = yr2 - yr1 + 1;\n    [n, m] = size(gpp);\n    disp_step = fix(n/100);\n    \n    parfor i = 1 : n\n        y1 = gpp(i, :);\n        y2 = y1(y1 >= xmin & y1 <= xmax);\n        y3 = y2';sy = sum(y3);\n        \n        nd = length(y1);\n        ns = length(y2);\n        y_max = max(y2);\n        y_min = min(y2);\n        \n        if   ns == nd && y_max > y_min\n            y4 = eemd(y3, 0.01, 100);\n            y5 = y4(:,end);\n            y6 = y5 - y5(1,1);\n            y7 = diff(y6);\n            y8 = mean(y7);\n            \n            yn = (y3 - y_min) / (y_max - y_min);\n            yn4 = eemd(yn, 0.01, 100);\n            yn5 = yn4(:,end);\n            yn6 = yn5 - yn5(1,1);\n            yn7 = diff(yn6);\n            yn8 = mean(yn7);\n            \n            p = 1 - normcdf(yn8, rand_mn, rand_sd);\n        else\n            y8 = -9999;\n            yn8 = -9999;\n            p = -9999;\n        end\n        \n        X(i, :) = [y8, yn8, p];\n        \n        if mod(i,disp_step)==0\n            disp(['Finished',num2str(i/n*100,'%.2f'),'%for block',num2str(bx)]);\n        end\n    end\n    \n    outf = [sub_raw, filesep, veg_index, '_EEMD_Trend', '_' , num2str(bx),'.flt']; \n    fop = fopen(outf, 'w');\n    fwrite(fop, X, 'float32');\n    fclose(fop);\n    \n    disp(outf);\n    \n    t2 = toc;\n    \n    disp(['Used time ', num2str(t2 * bx / 60, '%.2f'), ' min.']);\n    \n    disp(['Total need ', num2str(t2 * n_block / 3600, '%.2f'), ' hour']);\n    \n    xx = X(X > -900);\n    if ~isempty(xx)\n        X_mn(bx,:) = [mean(xx), std(xx), max(xx), min(xx)];\n    end\nend\nbox(X_mn(:,1));\n\ntic\nfor bx = 1 : n_block\n    file =[sub_raw, filesep, veg_index, '_EEMD_Trend', '_' , num2str(bx),'.flt']; \n    fip = fread(file, 'r');\n    dat = fread(fip, [Np(bx,1), 3], 'float32');\n    fclose(fip);\n    \n    if bx == 1\n        X = dat;\n    else\n        X = [X;